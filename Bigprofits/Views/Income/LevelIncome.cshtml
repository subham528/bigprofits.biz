@using System.Data
@model DataSet
@using Bigprofits.Common;
@{
    ViewData["Title"] = "Matching Income";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-12 col-sm-12">
        <div class="card ">
            <div class="card-header">
                <h3 class="card-title mb-0"> Matching Income</h3>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table id="data-table" class="table table-bordered text-nowrap mb-0">
                        <thead>
                            <tr>

                                <th>
                                    Sno
                                </th>
                               
                                <th>
                                   Matching
                                </th> 
                              
                                <th>
                                    Rate
                                </th> 
                                <th>
                                    Amount
                                </th>
                               
                               

                                <th>
                                    Date
                                </th>

                            </tr>
                        </thead>
                        <tbody>
                            @{
                                int sno2 = 1;
                            }

                            @foreach (DataRow row in Model.Tables[0].Rows)
                            {
                                <tr>
                                    <td>@sno2</td>
                                   
                                         <td>@("$" + Convert.ToDecimal(row["bnryMatch"]).ToString("0.00"))</td> 

                                   
                                   <td>@(Convert.ToDecimal(row["rpcharge"]).ToString("0.00") + "%")</td>
                                    <td>@("$" + Convert.ToDecimal(row["bnryAmount"]).ToString("0.00"))</td>



                                    <td>@row["bnryCDate"]</td>

                                   
                               
                                   
                                

                                </tr>
                                sno2++;
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div><!-- COL END -->
</div><!-- ROW-5 END -->
