    @* ============= Sweet Alert ==============*@
    @if (TempData["Ssuc"] != null)
    {
        <script>
            Swal.fire({
                icon: 'success',
                title: 'Success',
                text: '@TempData["Ssuc"]',
            })
        </script>
    }
    @if (TempData["Serr"] != null)
    {
        <script>
            Swal.fire({
                icon: 'error',
                title: 'Oops...',
                text: '@TempData["err"]',
            })
        </script>
    }
    @if (TempData["Sinfo"] != null)
    {
        <script>
            Swal.fire({
                icon: 'info',
                title: 'Oops...',
                text: '@TempData["info"]',
            })
        </script>
    }

    @* ============= CuteAlert ==============*@
    @if (TempData["Csuc"] != null)
    {
        <script>
            cuteAlert({
                type: "success",
                title: "Success..!!",
                message: "@TempData["Csuc"]",
                buttonText: "OK",
            @* closeStyle: "circle",*@
            @* onClose: function () {
            // Callback function executed when the alert is closed
            console.log("Alert closed.");
            }*@
                        });
        </script>
    }
    @if (TempData["Cerr"] != null)
    {
        <script>
            cuteAlert({
                type: "error",
                title: "Oops..!!",
                message: "@TempData["Cerr"]",
                buttonText: "OK",
            });
        </script>
    }
    @if (TempData["Cwar"] != null)
    {
        <script>
            cuteAlert({
                type: "warning",
                title: "Ooo..!!",
                message: "@TempData["Cwar"]",
                buttonText: "OK",
            });
        </script>
    }

    @* ============= Toastfy ==============*@
    @if (TempData["Tsuc"] != null)
    {
        <script>
            cuteToast({
                type: "success",
                message: "@TempData["Tsuc"]",
                timer: 5000,
            });
        </script>
    }
    @if (TempData["Terr"] != null)
    {
        <script>
            cuteToast({
                type: "error",
                message: "@TempData["Terr"]",
                timer: 5000,
            });
        </script>
    }
    @if (TempData["Twar"] != null)
    {
        <script>
            cuteToast({
                type: "warning",
                message: "@TempData["Twar"]",
                timer: 5000,
            });
        </script>
    }

